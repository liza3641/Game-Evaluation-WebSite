<!DOCTYPE html>
<html>
 <head>
 <title>게시판</title>
 <link rel='stylesheet' href='/stylesheets/liststyle.css' />
 </head>
 <body>
   <script type="text/javascript">
   /* Demo purposes only */
$(".hover").mouseleave(
function () {
  $(this).removeClass("hover");
}
);
   </script>
   
   <br><br><br><br>
   <% include index %>
   <h1><center>게임 목록</center></h1>
<div class="parent">
 <div class="child left">
   <% jdata.forEach(function(k){ if(k.id%3 == 0){ %>
   <figure class="imagelist" >
     <img src=<%= k.image %> alt="sample45" align="left">
     <figcaption>
       <h3><%= k.title %></h3>
       <p>정가<br>  <%= k.price %></p>
     </figcaption>
     <a href="/game/<%= k.id %>"></a>
   </figure>
  <% }}) %>
</div>

 <div class="child center">
   <% jdata.forEach(function(k){ if(k.id%3 == 1){ %>
   <figure class="imagelist" >
     <img src=<%= k.image %> alt="sample77" align="center">
     <figcaption>
       <h3><%= k.title %></h3>
       <p>정가<br>  <%= k.price %></p>
     </figcaption>
     <a href="/game/<%= k.id %>"></a>
   </figure>
  <% }}) %>
 </div>

 <div class="child right">
    <% jdata.forEach(function(k){ if(k.id%3 == 2){ %>
      <figure class="imagelist" >
        <img src=<%= k.image %> alt="sample99" align="center">
        <figcaption>
          <h3><%= k.title %></h3>
          <p>정가<br>  <%= k.price %></p>
        </figcaption>
        <a href="/game/<%= k.id %>"></a>
      </figure>
     <% }}) %>
  </div>
</div>


  </body>
</html>
